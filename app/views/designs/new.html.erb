<% provide(:title, 'Create Design') %>
<h1>Create Design</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for @design, :html => {:multipart => true} do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      
      <%= f.file_field :design %>
      
      <%= f.submit "Create", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
